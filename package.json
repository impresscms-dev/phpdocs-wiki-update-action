{
  "name": "phpdocs-wiki-update-action",
  "version": "0.0.0",
  "description": "This action generate documentation for PHP project and updates wiki.",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:imponeer/phpdocs-wiki-update-action.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "all": "npm install && npm run build && npm run format && npm run lint && npm run pack",
    "fast-all": "npm install && npm run build && npm run pack"
  },
  "keywords": [
    "github actions",
    "phpdocs",
    "wiki",
    "php project",
    "documentation",
    "phpdoc-md"
  ],
  "author": "Imponeer",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "@vercel/ncc": "^0.34.0",
    "child_process": "^1.0.2",
    "flow-bin": "^0.181.2",
    "picomatch": "^2.3.1",
    "recursive-readdir-sync": "^1.0.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "semver": "^7.3.7",
    "typescript": "^4.7.3"
  }
}
